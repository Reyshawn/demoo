{
  "name": "@fete/hooks",
  "version": "0.0.3",
  "description": "Some useful react hooks.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup src --dts"
  },
  "keywords": [
    "react",
    "hooks"
  ],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  }
}
